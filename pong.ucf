	 NET "vga_R[0]"            LOC = P103  | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;

    NET "vga_G[2]"           LOC = P102   | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
	 NET "vga_G[1]"           LOC = P101   | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
	 NET "vga_G[0]"           LOC = P99   | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
	 
    NET "vga_B[1]"             LOC = P98  | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
	 NET "vga_B[0]"             LOC = P96  | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
	 
	 NET "player_input[0]"          LOC = P80   | PULLUP  | IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 12;
